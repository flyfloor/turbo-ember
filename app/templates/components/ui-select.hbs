<label>{{label}}</label>
<div>
    <select class="ui dropdown {{search}}">
        {{#if allowBlank}}
            <option value="">{{placeHolder}}</option>
        {{/if}}
        {{#each options as |option|}}
            <option value="{{option.value}}">{{option.name}}</option>
        {{/each}}
    </select>
</div>